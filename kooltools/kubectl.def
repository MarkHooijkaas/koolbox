init_vars() {
    calc_transformed_os_arch
    download_filename=kubectl
    download_url="https://dl.k8s.io/release/${KOOLBOX_INSTALL_KUBECTL_VERSION}/bin/${OS}/${ARCH}/kubectl"
}

extract_downloaded_file() {
    dry-run cp ${download_path} ${extract_path}
    dry-run chmod 755  ${extract_path}
}

install_extras() {
    dry-run ${extract_path} completion bash >${completion_path}
}
